ableErrorCount":0,"fixableWarningCount":0},"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/index.js",[],["108","109"],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/App.js",["110"],"import React from 'react';\nimport { Switch, Route, Link, Redirect } from 'react-router-dom';\nimport {connect} from 'react-redux';\nimport './App.css';\nimport HomePage from './pages/homepage/homepage.component';\nimport ShopPage from './pages/shop/shop.component';\nimport Header from './components/header/header.component';\nimport SignInAndSignUpPage from './pages/sign-in-and-sign-up/sign-in-and-sign-up.component';\nimport {auth, createUserProfileDocument} from './firebase/firebase.utils';\nimport {setCurrentUser} from './redux/user/user.actions';\n\nclass App extends React.Component {\n\n\n  unsubscribeFromAuth = null;\n\n  componentDidMount(){\n    const {setCurrentUser} = this.props;\n\n    this.unsubscribeFromAuth = auth.onAuthStateChanged(async userAuth => {\n \n      if(userAuth){\n        const userRef = await createUserProfileDocument(userAuth);\n        userRef.onSnapshot(snapshot => {\n          setCurrentUser({\n              id: snapshot.id,\n              ...snapshot.data()\n          })\n        })\n        \n      }\n      setCurrentUser(userAuth);\n\n    } )\n  }\n\n  componentWillUnmount(){\n    this.unsubscribeFromAuth();\n  }\n\n  render(){\n    return (\n      <div>\n        <Header />\n        <Switch>\n          <Route exact path='/' component={HomePage} />\n          <Route path='/shop' component={ShopPage} />\n          <Route \n            exact \n            path='/signin' \n            render={()=> \n              this.props.currentUser ? (\n                <Redirect to='/' />\n              ) : (\n                <SignInAndSignUpPage />\n              )\n              } />\n        </Switch>\n      </div>\n    );\n}\n}\n\nconst mapStateToProps = ({user}) => ({\n  currentUser: user.currentUser\n})\n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user=> dispatch(setCurrentUser(user))\n})\nexport default connect(\n    mapStateToProps,\n    mapDispatchToProps\n  )(App);\n","/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/pages/homepage/homepage.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/components/directory/directory.components.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/components/menu-item/menu-item.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/pages/shop/shop.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/pages/shop/shop.data.js",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/components/collection-preview/collection-preview.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/components/collection-item/collection-item.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/components/header/header.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/pages/sign-in-and-sign-up/sign-in-and-sign-up.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/components/sign-in/sign-in.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/components/form-input/form-input.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/components/custom-button/custom-button.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/firebase/firebase.utils.js",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/components/sign-up/sign-up.component.jsx",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/redux/store.js",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/redux/root-reducer.js",[],"/Users/cosimopalma/dev/corsi/React Developer/crwn-clothing/src/redux/user/user.reducer.js",[],"/Users